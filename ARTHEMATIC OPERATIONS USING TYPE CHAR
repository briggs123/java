import java.lang.*;
import java.util.Scanner;
public class Main 
{
    public static void main(String[] args)
    {
        int a='a';
        int b='b';
        System.out.println(a);
        System.out.println(b);
        System.out.println(a*b);
        System.out.println(a/b);
        System.out.println(a%b);
    }
}
